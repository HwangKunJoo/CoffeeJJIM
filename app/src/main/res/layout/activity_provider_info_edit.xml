<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:text="이름"
        android:id="@+id/textView14"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="18dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="70dp"
        android:gravity="center"/>

    <TextView
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:text="아이디"
        android:id="@+id/textView15"
        android:textSize="18dp"
        android:layout_below="@+id/textView14"
        android:layout_alignLeft="@+id/textView14"
        android:layout_alignStart="@+id/textView14"
        android:gravity="center"
        android:layout_marginTop="30dp"/>

    <TextView
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:text="비밀번호"
        android:id="@+id/textView16"
        android:textSize="18dp"
        android:layout_below="@+id/textView15"
        android:layout_alignLeft="@+id/textView15"
        android:layout_alignStart="@+id/textView15"
        android:gravity="center"
        android:layout_marginTop="30dp"/>

    <TextView
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:text="비밀번호 확인"
        android:id="@+id/textView17"
        android:textSize="18dp"
        android:layout_below="@+id/textView16"
        android:layout_alignLeft="@+id/textView16"
        android:layout_alignStart="@+id/textView16"
        android:gravity="center"
        android:layout_marginTop="30dp"/>

    <TextView
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:text="휴대폰"
        android:id="@+id/textView18"
        android:textSize="18dp"
        android:layout_below="@+id/textView17"
        android:layout_alignLeft="@+id/textView17"
        android:layout_alignStart="@+id/textView17"
        android:gravity="center"
        android:layout_marginTop="30dp"/>

    <TextView
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:text="이메일"
        android:id="@+id/textView33"
        android:textSize="18dp"
        android:layout_below="@+id/textView18"
        android:layout_alignLeft="@+id/textView18"
        android:layout_alignStart="@+id/textView18"
        android:gravity="center"
        android:layout_marginTop="30dp"/>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/linearLayout">

        <Button
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="로그아웃"
            android:id="@+id/button6"
            android:layout_weight="1"/>

        <Button
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="완료"
            android:id="@+id/button7"
            android:layout_weight="1"/>
    </LinearLayout>

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText4"
        android:layout_alignTop="@+id/textView14"
        android:layout_toRightOf="@+id/textView14"
        android:layout_toEndOf="@+id/textView14"
        android:layout_above="@+id/textView15"
        android:layout_marginLeft="15dp"/>

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText5"
        android:layout_alignTop="@+id/textView15"
        android:layout_toRightOf="@+id/textView15"
        android:layout_toEndOf="@+id/textView15"
        android:layout_above="@+id/textView16"
        android:layout_marginLeft="15dp"/>

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText6"
        android:layout_alignTop="@+id/textView16"
        android:layout_alignLeft="@+id/editText5"
        android:layout_alignStart="@+id/editText5"
        android:layout_above="@+id/textView17" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText7"
        android:layout_alignTop="@+id/textView17"
        android:layout_alignLeft="@+id/editText6"
        android:layout_alignStart="@+id/editText6"
        android:layout_above="@+id/textView18" />

    <EditText
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText9"
        android:layout_alignTop="@+id/textView33"
        android:layout_alignBottom="@+id/textView33"
        android:layout_alignLeft="@+id/editText7"
        android:layout_alignStart="@+id/editText7" />

    <Spinner
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/spinner"
        android:layout_alignTop="@+id/textView18"
        android:layout_alignLeft="@+id/editText7"
        android:layout_alignStart="@+id/editText7" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/textView34"
        android:layout_alignTop="@+id/spinner"
        android:layout_toRightOf="@+id/spinner"
        android:layout_toEndOf="@+id/spinner"
        android:layout_alignBottom="@+id/textView18"
        android:textSize="18dp"
        android:layout_marginLeft="5dp"/>

    <EditText
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText8"
        android:layout_alignTop="@+id/textView34"
        android:layout_toRightOf="@+id/textView34"
        android:layout_toEndOf="@+id/textView34"
        android:layout_alignBottom="@+id/textView34"
        android:layout_marginLeft="5dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/textView35"
        android:textSize="18dp"
        android:layout_alignTop="@+id/editText8"
        android:layout_toRightOf="@+id/editText8"
        android:layout_toEndOf="@+id/editText8"
        android:layout_marginLeft="5dp"/>

    <EditText
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/editText10"
        android:layout_alignTop="@+id/textView35"
        android:layout_toRightOf="@+id/textView35"
        android:layout_toEndOf="@+id/textView35"
        android:layout_above="@+id/textView33"
        android:layout_marginLeft="5dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="\@"
        android:id="@+id/textView36"
        android:layout_alignTop="@+id/editText9"
        android:layout_toRightOf="@+id/editText9"
        android:layout_toEndOf="@+id/editText9"
        android:textSize="18dp"
        android:layout_marginLeft="5dp"/>

    <Spinner
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:id="@+id/spinner2"
        android:layout_alignBottom="@+id/textView36"
        android:layout_toRightOf="@+id/editText8"
        android:layout_toEndOf="@+id/editText8"
        android:layout_marginLeft="5dp"/>

</RelativeLayout>