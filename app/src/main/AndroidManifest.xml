<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.coffeejjim.developers">

    <application
        android:name=".CoffeeJJIMApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomeActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".estimate.EstimateSheetActivity" />
        <activity android:name=".login.FindInfoActivity" />
        <activity
            android:name=".login.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CoffeeJJIMSplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CafeReservationActivity" />
        <activity android:name=".AutionProcessActivity" />
        <activity android:name=".EventDetailActivity"></activity>
        <activity android:name=".CafeDetailActivity"/>
        <activity android:name=".ExtraFunctionsActivity"/>
        <activity android:name=".LikeListActivity"/>
        <activity android:name=".AllCafeListActivity"/>
    </application>

</manifest>